{
  "name": "verman-cli",
  "version": "1.0.0",
  "description": "A CLI library for managing node/npm versions range engine limits.",
  "main": "./src/index.js",
  "bin": {
    "verman": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./src/index.js",
    "test": "node ./test/run.js",
    "test:report": "npx c8 -c=test/.c8rc -- npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylarcher/verman-cli.git"
  },
  "keywords": [
    "cli",
    "command-line interface",
    "compatibility",
    "engines",
    "library",
    "limits",
    "maximum",
    "minimum",
    "node",
    "nodejs",
    "npm",
    "range",
    "support",
    "tool",
    "version"
  ],
  "author": "Dylan Archer <dylarcher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dylarcher/verman-cli/issues"
  },
  "homepage": "https://dylarcher.github.io/verman-cli/",
  "devDependencies": {
    "c8": "10.1.3",
    "commander": "14"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8.6"
  },
  "packageManager": "npm@8.6.0"
}
